---
import "@fontsource/jost";
import '@/styles/globals.css';

import { SEO } from "astro-seo";

import { Navbar } from "@/components/Navbar";

const { title } = Astro.props;
---

<html lang="en">
	<head>
    <SEO
      title="Remove AI Overview - Browser Extension"
      description="Remove AI Overview is an open source browser extension for remove AI Overview sections from Google search results."
      openGraph={{
        basic: {
          title: "Remove AI Overview",
          type: "Browser Extension",
          image: "",
        }
      }}
    />
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/removeaioverviewlogo.png" />
    <link rel="sitemap" href="/sitemap-index.xml" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="bg-[#09090B] text-white">
    <Navbar />
    <slot />
	</body>
</html>
